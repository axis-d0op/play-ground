{
  "extends": ["config:base", "schedule:nonOfficeHours"],
  "enabledManagers": ["npm"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "npm": {
    "commitMessagePrefix": "chore: ",
    "encrypted": {
      "npmToken": ""
    },
    "npmrc": "@lkp-rnd:registry=https://npm.pkg.github.com/\n@axteams-software:registry=https://npm.pkg.github.com/\n//npm.pkg.github.com/:_authToken=${NPM_TOKEN}\nauto-install-peers=true",
    "packageRules": [
      {
        "matchPackagePatterns": ["@fluentui/react-components\\/"],
        "enabled": false
      }
    ],
    "major": {
      "automerge": false,
      "draftPR": true
    },
    "patch": {
      "automerge": false
    },
    "minor": {
      "automerge": false
    },
    "pin": {
      "automerge": false
    }
  },
  "updateNotScheduled": false,
  "prConcurrentLimit": 15,
  "stabilityDays": 1,
  "timezone": "Europe/Stockholm",
  "rebaseWhen": "behind-base-branch"
}
